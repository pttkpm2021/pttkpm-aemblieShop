            <img src="/images/icons/manu/Anker58-b_19.png" alt="Phụ kiện Anker">
        </a>
        <a href="/phu-kien-xmobile<%= filter ? '/?filter=' + filter : '' %>" title="Phụ kiện Xmobile" class="icon-img 
            <%= brand === "xmobile" ? 'check' : '' %>">
            <img src="/images/icons/manu/Xmobile58-b_43.jpg" alt="Phụ kiện Xmobile">
        </a>
        <a href="/phu-kien-ava<%= filter ? '/?filter=' + filter : '' %>" title="Phụ kiện Ava" class="icon-img 
            <%= brand === "ava" ? 'check' : '' %>">
            <img src="/images/icons/manu/AVA58-b_51.jpg" alt="Phụ kiện Ava">
        </a>
        <a href="/phu-kien-evalu<%= filter ? '/?filter=' + filter : '' %>" title="Phụ kiện Evalu" class="icon-img 
            <%= brand === "evalu" ? 'check' : '' %></a>">
            <img src="/images/icons/manu/eValu58-b_21.jpg" alt="Phụ kiện Evalu">
        </a>
    </div>
    <div class="sort-container clearfix">
        <% if (brand == null){ %>
            <span>Chọn mức giá:</span>
            <a class="bestsellers_sorting <%= filter=='duoi-1-tram' ? 'filter_selected' : '' %>" href="/phu-kien/?filter=duoi-1-tram" rel="nofollow">
                Dưới 1 trăm
            </a>
            <a class="bestsellers_sorting <%= filter=='tu-1-3-tram' ? 'filter_selected' : '' %>" href="/phu-kien/?filter=tu-1-3-tram" rel="nofollow">
                Từ 1 trăm - 3 trăm
            </a>
            <a class="bestsellers_sorting <%= filter=='tu-3-5-tram' ? 'filter_selected' : '' %>" href="/phu-kien/?filter=tu-3-5-tram" rel="nofollow">
                Từ 3 trăm - 5 trăm
            </a>
            <a class="bestsellers_sorting <%= filter=='tren-5-tram' ? 'filter_selected' : '' %>" href="/phu-kien/?filter=tren-5-tram" rel="nofollow">
                Trên 5 trăm
            </a>
        <% } else { %>
            <span>Chọn mức giá:</span>
            <a class="bestsellers_sorting <%= filter=='duoi-1-tram' ? 'filter_selected' : '' %>" href="/phu-kien-<%= brand %>/?filter=duoi-1-tram" rel="nofollow">
                Dưới 1 trăm
            </a>
            <a class="bestsellers_sorting <%= filter=='tu-1-3-tram' ? 'filter_selected' : '' %>" href="/phu-kien-<%= brand %>/?filter=tu-1-3-tram" rel="nofollow">
                Từ 1 trăm - 3 trăm
            </a>
            <a class="bestsellers_sorting <%= filter=='tu-3-5-tram' ? 'filter_selected' : '' %>" href="/phu-kien-<%= brand %>/?filter=tu-3-5-tram" rel="nofollow">
                Từ 3 trăm - 5 trăm
            </a>
            <a class="bestsellers_sorting <%= filter=='tren-5-tram' ? 'filter_selected' : '' %>" href="/phu-kien-<%= brand %>/?filter=tren-5-tram" rel="nofollow">
                Trên 5 trăm
            </a>
        <% } %>
        <div class="dropdown-container">
            <span id="sw_elm_sort_fields" class="sort-dropdown cm-combination">
                <a>
                    <tloc_sort>Sắp xếp</tloc_sort><i class="icon-down-micro"></i>
                </a>
            </span>
            <ul id="elm_sort_fields" class="dropdown-content cm-popup-box hidden">
                <li class="sort-by-popularity-desc ld-over-full" onclick="this.classList.toggle('running')">
                    <div class="ld lds-ellipsis"><div></div><div></div><div></div><div></div></div>
                    <a id="dropdown-item-common-desc" class="<%= (sort_by == 'popularity' && sort_order == 'desc') ? 'check' : '' %>" data-ca-target-id="pagination_contents"
                        rel="nofollow">
                    <i class="icontloc-checklist"></i>
                        Nổi bật nhất
                    </a>
                </li>
                <li class="sort-by-price-desc ld-over-full" onclick="this.classList.toggle('running')">
                    <div class="ld lds-ellipsis"><div></div><div></div><div></div><div></div></div>
                    <a id="dropdown-item-price-desc" class="<%= (sort_by == 'price' && sort_order == 'desc') ? 'check' : '' %>" data-ca-target-id="pagination_contents" 
                    rel="nofollow">
                    <i class="icontloc-checklist"></i>
                        Giá cao đến thấp
                    </a>
                </li>
                <li class="sort-by-price-asc ld-over-full" onclick="this.classList.toggle('running')">
                    <div class="ld lds-ellipsis"><div></div><div></div><div></div><div></div></div>
                    <a id="dropdown-item-price-asc" class="<%= (sort_by == 'price' && sort_order == 'asc') ? 'check' : '' %>" data-ca-target-id="pagination_contents" 
                        rel="nofollow">
                    <i class="icontloc-checklist"></i>
                        Giá thấp đến cao
                    </a>
                </li>
            </ul>
        </div>
        <% if (filter || brand != null){ %>
            <div class="da_chon_ac clearfix">
                <% if (brand != null){ %> 
                    <a href="javascript:;" onclick="location.href=`/phu-kien<%= (filter && brand != null) ? ('/?filter=' + filter) : '' %>`" class="bestsellers_sorting-ac">
                        <%= brand.charAt(0).toUpperCase() + brand.substring(1) %> <i class="uni-close"></i></a>
                <% } %>
                <% if (filter){ %> 
                    <a class="bestsellers_sorting-ac" href="javascript:;" onclick="location.href=`/phu-kien<%= brand != null ? '-' + brand : '' %>`" 
                            rel="nofollow"><%= filter_text %> <i class="uni-close"></i></a>
                <% } %>
                <% if (filter && brand != null){ %> 
                    <a class="bestsellers_sorting-ac ac-xoatatca" href="javascript:;" onclick="location.href=`/phu-kien`" 
                        rel="nofollow">Xóa tất cả <i class="uni-close"></i></a>
                <% } %>
                <h1 class="float-right">PHỤ KIỆN <%= (brand != null ? brand : '') + ' ' + (filter ? filter_text : '') %></h1>
            </div>
        <% } %>
    </div>
<% } %>